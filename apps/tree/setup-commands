# Clear previous setup
rm -rf apps/tree

# Create directory structure
mkdir -p apps/tree

# Create new app in the correct location
cd apps/tree
npx create-react-app .

# Install required dependencies
npm install rc-tree react-virtualized @aws-amplify/cli aws-amplify @emotion/styled

# Clear any potential cache issues
npm cache clean --force

# Reinstall dependencies
rm -rf node_modules
npm install 